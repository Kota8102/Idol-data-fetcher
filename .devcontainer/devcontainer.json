	"name": "AWS Lambda Node.js + Python",
	"build": {
		"dockerfile": "Dockerfile"
	},
"remoteUser": "node",
"features": {
	"ghcr.io/devcontainers/features/aws-cli:1": {},
	"ghcr.io/devcontainers-contrib/features/serverless:2": {}
},
"customizations": {
